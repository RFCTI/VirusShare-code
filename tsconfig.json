{
    "compilerOptions": {
      "target": "es5",
      "module": "es2020",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "lib": ["dom", "esnext"]
    },
    "include": ["src"]
  }
  