{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/old-url",
      "destination": "/new-url",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/some/old-path",
      "destination": "/some/new-path"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Custom-Header",
          "value": "my-value"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=0, must-revalidate, public"
        }
      ]
    }
  ],
  "env": {
    "MY_SECRET": "@my_secret"
  },
  "build": {
    "env": {
      "ANOTHER_SECRET": "@another_secret"
    }
  }
}
